<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="reviewDao">
	
	<select id="leadReview" parameterType="int" resultType="reviewDto">
		select * from review where bookNo = #{bookNo}
	</select>
	
	<insert id="insertView" parameterType="reviewDto">
		insert into review values(#{reviewNo},#{memberNo},
		#{reviewTitle},#{reviewFile},#{reviewCont},#{reviewRegdate},#{bookNo})
	</insert>

</mapper>